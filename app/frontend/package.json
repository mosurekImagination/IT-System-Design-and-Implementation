{
  "name": "ng-powierzenia3000",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "extract-i18n": "ng xi18n ng-powierzenia3000 --i18n-format xlf --output-path i18n --i18n-locale pl && ng run ng-powierzenia3000:xliffmerge",
    "start-en": "ng serve --configuration=en",
    "build-locale:en": "ng build --configuration=en --i18n-locale en --i18n-format xlf --i18n-file src/i18n/messages.en.xlf --output-path=dist/en --baseHref /en/",
    "build-locale:pl": "ng build --prod --i18n-locale pl --i18n-format xlf --i18n-file src/i18n/messages.pl.xlf --output-path=dist/pl --baseHref /pl/",
    "build-locale": "npm run build-locale:en && npm run build-locale:pl"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~7.2.0",
    "@angular/cdk": "~7.3.7",
    "@angular/common": "~7.2.0",
    "@angular/compiler": "~7.2.0",
    "@angular/core": "~7.2.0",
    "@angular/fire": "^5.3.0",
    "@angular/forms": "~7.2.0",
    "@angular/material": "^7.3.7",
    "@angular/platform-browser": "~7.2.0",
    "@angular/platform-browser-dynamic": "~7.2.0",
    "@angular/router": "~7.2.0",
    "@ngx-i18nsupport/tooling": "^1.1.3",
    "@ngx-translate/i18n-polyfill": "^1.0.0",
    "bootstrap": "^4.4.1",
    "core-js": "^2.5.4",
    "firebase": "^7.7.0",
    "hammerjs": "^2.0.8",
    "is-docker": "^2.0.0",
    "jquery": "^3.4.1",
    "popper": "^1.0.1",
    "rxjs": "~6.3.3",
    "tslib": "^1.9.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.13.4",
    "@angular/cli": "~7.3.8",
    "@angular/compiler-cli": "~7.2.0",
    "@angular/language-service": "~7.2.0",
    "@ngx-i18nsupport/ngx-i18nsupport": "^1.1.3",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.5.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "node-sass": "^4.13.1",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.2.2"
  }
}
